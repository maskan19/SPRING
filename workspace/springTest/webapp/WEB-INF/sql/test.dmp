CREATE TABLE EMPLOYEE 
(
  EMPLOYEE_ID VARCHAR2(40) NOT NULL 
, EMPLOYEE_AUTHORITY VARCHAR2(20) DEFAULT 'ROLE_USER'
, EMPLOYEE_PICTURE VARCHAR2(200) DEFAULT 'noimg.jpg '
, EMPLOYEE_PWD VARCHAR2(40) 
, EMPLOYEE_NAME VARCHAR2(15) 
, EMPLOYEE_PHONE VARCHAR2(20) NOT NULL 
, EMPLOYEE_EMAIL VARCHAR2(60) 
, CONSTRAINT EMPLOYEE_PK PRIMARY KEY 
  (
    EMPLOYEE_ID 
  )
  ENABLE 
)